<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
  package="com.cto3543.exokotlin">

  <uses-permission android:name="android.permission.INTERNET"/>

  <application
    android:allowBackup="true"
    android:icon="@mipmap/ic_launcher"
    android:label="@string/app_name"
    android:name=".ExoKotlinApp"
    android:roundIcon="@mipmap/ic_launcher_round"
    android:supportsRtl="true"
    android:theme="@style/AppTheme">
    <activity android:name=".groupmarket.GroupMarketActivity">
    </activity>
    <activity android:name=".menu.MenuActivity">
      <intent-filter>
        <action android:name="android.intent.action.MAIN"/>

        <category android:name="android.intent.category.LAUNCHER"/>
      </intent-filter>
    </activity>
    <activity android:name=".market.MarketActivity">
    </activity>
    <activity android:name=".detail.DetailsActivity">
    </activity>
    <activity android:name=".block.BlockActivity">
    </activity>
    <activity android:name=".bolivar.BolivarActivity"
      android:label="@string/api">
    </activity>
  </application>

</manifest>