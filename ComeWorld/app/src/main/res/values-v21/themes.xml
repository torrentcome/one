<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">

    <style name="Theme.TranslucentActionBar" parent="Theme.AppCompat" tools:ignore="NewApi">
        <item name="android:actionBarStyle" tools:ignore="NewApi">@style/Widget.AppCompat.ActionBar</item>
    </style>

    <style name="Theme.TranslucentActionBar.ActionBar" />

    <style name="Theme.TranslucentActionBar.ActionBar.Overlay">
